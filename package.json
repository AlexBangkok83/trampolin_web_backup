{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:main": "PORT=3001 next dev",
    "dev:app": "PORT=3002 next dev",
    "build": "next build",
    "seed": "npx ts-node prisma/seed.ts",
    "start": "next start",
    "prepare": "husky install",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "format": "prettier --write .",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up",
    "docker:up-build": "docker compose up --build",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:migrate": "docker compose exec web npx prisma migrate deploy",
    "docker:studio": "docker compose exec web npx prisma studio",
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "docker:dev-build": "docker compose -f docker-compose.dev.yml up --build",
    "docker:dev-down": "docker compose -f docker-compose.dev.yml down"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@stripe/stripe-js": "^7.9.0",
    "@types/pg": "^8.15.5",
    "bcrypt": "^6.0.0",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "stripe": "^18.5.0",
    "zod": "^3.23.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@prisma/client": "^6.15.0",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "husky": "^8.0.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.15.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  }
}
